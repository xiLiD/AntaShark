@keyframes snow1 {
  0% {
    top: 0;
    right: 0;
    opacity: 1;
    width: 80rpx;
    height:80rpx;
    transform: rotate(0deg);
  }

  100% {
    right: 700rpx;
    top: 1200rpx;
    width: 70rpx;
    height:70rpx;
    opacity: 0;
    transform: rotate(1080deg);
  }
}
@keyframes snow2 {
  0% {
    top: 600rpx;
    right: -50rpx;
    opacity: 1;
    width: 60rpx;
    height: 60rpx;
    transform: rotate(0deg);
  }

  100% {
    right: 700rpx;
    top: 1200rpx;
    width: 50rpx;
    height: 50rpx;
    opacity: 0;
    transform: rotate(1080deg);
  }
}
